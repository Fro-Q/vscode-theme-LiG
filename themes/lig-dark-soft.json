  "tokenColors": [
    {
      "scope": [
        "entity.name.tag",
        "variable.parameter",
        "punctuation.definition.decorator"
      ],
      "settings": {
        "foreground": "#60b89a"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "keyword.control.flow",
        "storage.modifier.async",
        "storage.type.function.async",
        "punctuation.attribute-shorthand.bind"
      ],
      "settings": {
        "foreground": "#e0926d"
      }
    },
    {
      "scope": [
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#f6bf9f"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#6fa8f0"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.character.escape",
        "support.type.builtin",
        "string.regexp",
        "entity.name.type.interface",
        "entity.name.type.alias",
        "variable.parameter.function.language.special"
      ],
      "settings": {
        "foreground": "#aacdf7"
      }
    },
    {
      "scope": [
        "support.type.primitive",
        "support.type"
      ],
      "settings": {
        "foreground": "#3f6496"
      }
    },
    {
      "scope": [
        "constant.character",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#b7b3b0"
      }
    },
    {
      "scope": [
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#fff9f5"
      }
    },
    {
      "scope": [
        "string",
        "comment",
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#6f6d6c"
      }
    }
  ],
  "rules": [
    {
      "token": "entity.name.tag",
      "foreground": "60b89a"
    },
    {
      "token": "variable.parameter",
      "foreground": "60b89a"
    },
    {
      "token": "punctuation.definition.decorator",
      "foreground": "60b89a"
    },
    {
      "token": "entity.name.function",
      "foreground": "e0926d"
    },
    {
      "token": "keyword.control.flow",
      "foreground": "e0926d"
    },
    {
      "token": "storage.modifier.async",
      "foreground": "e0926d"
    },
    {
      "token": "storage.type.function.async",
      "foreground": "e0926d"
    },
    {
      "token": "punctuation.attribute-shorthand.bind",
      "foreground": "e0926d"
    },
    {
      "token": "support.function.builtin",
      "foreground": "f6bf9f"
    },
    {
      "token": "constant.numeric",
      "foreground": "6fa8f0"
    },
    {
      "token": "constant.language.boolean",
      "foreground": "6fa8f0"
    },
    {
      "token": "support.class.component",
      "foreground": "6fa8f0"
    },
    {
      "token": "constant.language",
      "foreground": "aacdf7"
    },
    {
      "token": "constant.character.escape",
      "foreground": "aacdf7"
    },
    {
      "token": "support.type.builtin",
      "foreground": "aacdf7"
    },
    {
      "token": "string.regexp",
      "foreground": "aacdf7"
    },
    {
      "token": "entity.name.type.interface",
      "foreground": "aacdf7"
    },
    {
      "token": "entity.name.type.alias",
      "foreground": "aacdf7"
    },
    {
      "token": "variable.parameter.function.language.special",
      "foreground": "aacdf7"
    },
    {
      "token": "support.type.primitive",
      "foreground": "3f6496"
    },
    {
      "token": "support.type",
      "foreground": "3f6496"
    },
    {
      "token": "constant.character",
      "foreground": "b7b3b0"
    },
    {
      "token": "entity.other.attribute-name",
      "foreground": "b7b3b0"
    },
    {
      "token": "variable.other.readwrite",
      "foreground": "fff9f5"
    },
    {
      "token": "string",
      "foreground": "6f6d6c"
    },
    {
      "token": "comment",
      "foreground": "6f6d6c"
    },
    {
      "token": "keyword.operator",
      "foreground": "6f6d6c"
    },
    {
      "token": "punctuation",
      "foreground": "6f6d6c"
    }
  ]
}
